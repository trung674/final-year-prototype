{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
      "linebreak-style": 0,
      "no-console": 0,
      "indent": [ 2, 4 ],
      "no-multiple-empty-lines": [ 2, { "max": 1, "maxEOF": 0, "maxBOF": 0 } ],
      "prefer-const": 1
    }
}
